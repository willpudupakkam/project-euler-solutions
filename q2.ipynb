    "def fibonacci_evens(n):\n",
    "    evens_lst = [2]\n",
    "\n",
    "    x0 = 1\n",
    "    x1 = 2\n",
    "\n",
    "    while x0 + x1 < n:\n",
    "        x0_old = x0\n",
    "        if (x0 + x1) % 2 == 0:\n",
    "            evens_lst.append(x0 + x1)\n",
    "        x0 = x1\n",
    "        x1 = x0_old + x1\n",
    "\n",
    "    return evens_lst\n",
    "\n",
    "sum(fibonacci_evens(4000000))"
   ]
  }
